{
  "name": "@triviumsoftware/jumpstart",
  "type": "module",
  "version": "0.0.0",
  "description": "Jumpstart is a small, simple project generator based on my personal preferences.",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/jumpstart.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run barrels && npm test && tsc",
    "build:ci": "npm run clean && npm test && tsc",
    "clean": "del ./lib ./coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "dependencies": {
    "liquidjs": "10.16.3",
    "pkg-dir": "8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@faker-js/faker": "8.4.1",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/node": "20.14.8",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "@vitest/coverage-v8": "2.0.5",
    "del-cli": "5.1.0",
    "eslint": "8.57.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.9",
    "prettier": "3.3.3",
    "release-it": "17.6.0",
    "testdouble": "3.20.2",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  }
}
