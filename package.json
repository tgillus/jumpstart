{
  "name": "@triviumsoftware/jumpstart",
  "type": "module",
  "version": "0.0.0",
  "description": "Jumpstart is a small, simple project generator based on my personal preferences.",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/jumpstart.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run barrels && npm test && tsc",
    "build:ci": "npm run clean && npm test && tsc",
    "clean": "del ./lib ./coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "dependencies": {
    "liquidjs": "10.19.1",
    "pkg-dir": "8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/js": "9.17.0",
    "@faker-js/faker": "9.3.0",
    "@release-it/conventional-changelog": "9.0.3",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.10.2",
    "@vitest/coverage-v8": "2.1.8",
    "del-cli": "6.0.0",
    "eslint": "9.17.0",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "prettier": "3.4.2",
    "release-it": "17.10.0",
    "testdouble": "3.20.2",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.1",
    "vitest": "2.1.8"
  }
}
